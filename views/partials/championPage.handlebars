	<head>
		
	<meta charset="utf-8">
	<link href="https://fonts.googleapis.com/css?family=Roboto:100,400,700" rel="stylesheet">
	<link href="Champion.css" rel="stylesheet" type="text/css">
	
	</head>
	

	<header>
		<nav class="nav"><a href="../index.html" class="return">All Champions</a></nav>
	</header>
	
	<body>
		<div id="ChampionInfo">
			<img src="/Champions/Champion-Images/{{image}}" alt="{{name}}" class="image border"/>
			<h1 class="Name border">{{name}}</h1>
			<p class="Lore border">{{lore}}</p>
		</div>
		
		<div class ="Moves-Container">
			<article id = "passive-container" class="move">
				<div class="border">
					<p id = "passive">
						Passive
					</p>
				</div>
				
				<div class="description border" id="passiveborder">
					<p class = "move-name">Passive's Name</p>
					<p class="move-desc">
						{{moves[passive]}}
					</p>
				</div>
			</article>
		</div>
			
		<h2 id="ChampionMove" class="border">Champion Name's Move Set</h2>
		

		<main class="Moves-Container">
			<article class="move">
				<div class="key border">
					<p class="keystroke">
						Q
					</p>
				</div>
				
				<div class="description border">
					<p class = "move-name">Q Move's Name</p> <!--Replace with the move's name-->
					<p class="move-desc">
						{{moves[q]}}
					</p>
				</div>
			</article>
			
			<article class="move">
				<div class="key border">
					<p class="keystroke">
						W
					</p>
				</div>
				
				<div class="description border">
					<p class = "move-name">W Move's Name</p><!--Replace with the move's name-->
					<p class="move-desc">
						{{moves[w]}}
					</p>
				</div>
			</article>
			
			<article class="move">
				<div class="key border">
					<p class="keystroke">
						E
					</p>
				</div>
				
				<div class="description border">
					<p class = "move-name">E Move's Name</p><!--Replace with the move's name-->
					<p class="move-desc">
						{{moves[e]}}
					</p>
				</div>
			</article>
			
			<article class="move">
				<div class="key border">
					<p class="keystroke">
						R
					</p>
				</div>
				
				<div class="description border">
					<p class = "move-name">R Move's Name</p><!--Replace with the move's name-->
					<p class="move-desc">
						{{moves[r]}}
					</p>
				</div>
			</article>
			
		</main>
    </body>